    <xs:complexType name="expired_writeoff_notification">
        <xs:annotation>
            <xs:documentation>Уведомление об автоматическом списании ЛП с истекшим сроком годности</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="subject_id" type="subject_any_type">
